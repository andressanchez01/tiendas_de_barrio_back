<h1>Editar Usuario</h1>
<%= form_for @user do |f| %>
  <div class="field">
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :phone %>
    <%= f.text_field :phone %>
  </div><div class="field">
  <%= f.label :address %>
  <%= f.text_field :address %>
</div>
  <div class="field">
  <%= f.label :fiscal_number %>
  <%= f.text_field :fiscal_number %>
</div>
  <div class="field">
    <%= f.label :role %>
    <%= f.text_field :role %>
  </div>
  <div class="actions">
    <%= f.submit 'Actualizar Usuario' %>
  </div>
<% end %>